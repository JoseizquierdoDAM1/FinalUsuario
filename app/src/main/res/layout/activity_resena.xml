<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResenaActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:textAlignment="center">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="461dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="287dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/labelNombreRestaurante"
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    tools:layout_editor_absoluteX="96dp"
                    tools:layout_editor_absoluteY="49dp" />

                <ImageView
                    android:id="@+id/labelImagenRestaurante"
                    android:layout_width="match_parent"
                    android:layout_height="162dp"
                    tools:layout_editor_absoluteX="92dp"
                    tools:layout_editor_absoluteY="87dp"
                    tools:srcCompat="@tools:sample/avatars" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="273dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical"></LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="251dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginStart="70dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:textAlignment="center">

                    <ImageView
                        android:id="@+id/imageView11"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:onClick="unaestrella"
                        android:visibility="visible"
                        app:srcCompat="@android:drawable/btn_star_big_off"
                        tools:layout_editor_absoluteX="68dp"
                        tools:layout_editor_absoluteY="264dp" />

                    <ImageView
                        android:id="@+id/imageView12"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:onClick="dosestrellas"
                        android:visibility="visible"
                        app:srcCompat="@android:drawable/btn_star_big_off"
                        tools:layout_editor_absoluteX="110dp"
                        tools:layout_editor_absoluteY="264dp" />

                    <ImageView
                        android:id="@+id/imageView13"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:onClick="tresestrellas"
                        android:visibility="visible"
                        app:srcCompat="@android:drawable/btn_star_big_off"
                        tools:layout_editor_absoluteX="151dp"
                        tools:layout_editor_absoluteY="264dp" />

                    <ImageView
                        android:id="@+id/imageView14"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:onClick="cuatroestrellas"
                        android:visibility="visible"
                        app:srcCompat="@android:drawable/btn_star_big_off"
                        tools:layout_editor_absoluteX="193dp"
                        tools:layout_editor_absoluteY="264dp" />

                    <ImageView
                        android:id="@+id/imageView15"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:onClick="cincoestrellas"
                        app:srcCompat="@android:drawable/btn_star_big_off"
                        tools:layout_editor_absoluteX="234dp"
                        tools:layout_editor_absoluteY="264dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/Estrella1"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:onClick="unaestrella"
                        android:visibility="invisible"
                        app:srcCompat="@android:drawable/btn_star_big_on"
                        tools:layout_editor_absoluteX="152dp"
                        tools:layout_editor_absoluteY="264dp" />

                    <ImageView
                        android:id="@+id/Estrella2"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:onClick="dosestrellas"
                        android:visibility="invisible"
                        app:srcCompat="@android:drawable/btn_star_big_on"
                        tools:layout_editor_absoluteX="235dp"
                        tools:layout_editor_absoluteY="264dp" />

                    <ImageView
                        android:id="@+id/Estrella3"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:onClick="tresestrellas"
                        android:visibility="invisible"
                        app:srcCompat="@android:drawable/btn_star_big_on"
                        tools:layout_editor_absoluteX="111dp"
                        tools:layout_editor_absoluteY="264dp" />

                    <ImageView
                        android:id="@+id/Estrella4"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:onClick="cuatroestrellas"
                        android:visibility="invisible"
                        app:srcCompat="@android:drawable/btn_star_big_on"
                        tools:layout_editor_absoluteX="69dp"
                        tools:layout_editor_absoluteY="264dp" />

                    <ImageView
                        android:id="@+id/Estrella5"
                        android:layout_width="45dp"
                        android:layout_height="45dp"
                        android:onClick="cincoestrellas"
                        android:visibility="invisible"
                        app:srcCompat="@android:drawable/btn_star_big_on"
                        tools:layout_editor_absoluteX="193dp"
                        tools:layout_editor_absoluteY="264dp" />

                </LinearLayout>

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="779dp"
            android:layout_weight="1.6"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Escribe una rese単a"
                android:textAlignment="center" />

            <EditText
                android:id="@+id/labelRese単a"
                android:layout_width="match_parent"
                android:layout_height="92dp"
                android:ems="10"
                android:gravity="start|top"
                android:inputType="textMultiLine"
                tools:layout_editor_absoluteX="65dp"
                tools:layout_editor_absoluteY="312dp" />

            <Button
                android:id="@+id/button6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="crearRese単a"
                android:text="A単adir reserva" />
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>